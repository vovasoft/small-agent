{
  "name": "农业总支出指标计算",
  "description": "计算农业领域的各项支出指标，包括生产资料支出、劳动力支出、土地租金等",
  "category": "农业经济",
  "version": "1.0.0",
  "author": "big_agent",
  "created_at": "2024-12-18",
  "keywords": ["农业", "支出", "指标", "经济", "成本"],
  "interference_field_1": "这是一个无关的干扰字段",
  "deprecated_config": {
    "old_version": "0.9.0",
    "migration_notes": "已废弃，请使用新版本"
  },
  "debug_info": {
    "test_mode": false,
    "mock_data_enabled": true
  },
  "unrelated_metadata": {
    "created_by": "developer_team",
    "reviewed_by": "qa_team",
    "last_modified": "2024-12-20"
  },
  "api_config": {
    "method": "POST",
    "url": "http://127.0.0.1:8000/agriculture/expense-calculation",
    "headers": {
      "Content-Type": "application/json"
    },
    "timeout": 30
  },
  "param_mapping": {
    "time_period": "time_period",
    "region": "region",
    "crop_type": "crop_type",
    "calculation_method": "calculation_method"
  },
  "input_schema": {
    "type": "object",
    "properties": {
      "time_period": {
        "type": "string",
        "description": "时间周期，如'2023'或'2023-Q1'"
      },
      "region": {
        "type": "string",
        "description": "地区名称"
      },
      "crop_type": {
        "type": "string",
        "description": "作物类型"
      },
      "calculation_method": {
        "type": "string",
        "enum": ["standard", "advanced", "custom"],
        "default": "standard"
      }
    },
    "required": ["time_period"]
  },
  "output_schema": {
    "type": "object",
    "properties": {
      "total_expense": {
        "type": "number",
        "description": "总支出金额"
      },
      "expense_breakdown": {
        "type": "object",
        "properties": {
          "production_materials": {"type": "number"},
          "labor_cost": {"type": "number"},
          "land_rent": {"type": "number"},
          "equipment_depreciation": {"type": "number"},
          "other_costs": {"type": "number"}
        }
      },
      "calculation_metadata": {
        "type": "object",
        "properties": {
          "method_used": {"type": "string"},
          "data_sources": {"type": "array"},
          "confidence_level": {"type": "number"}
        }
      }
    }
  },
  "calculation_logic": {
    "steps": [
      "数据收集和验证",
      "成本分类和汇总",
      "指标计算和标准化",
      "结果验证和格式化"
    ],
    "formulas": {
      "total_expense": "SUM(production_materials + labor_cost + land_rent + equipment_depreciation + other_costs)",
      "cost_efficiency_ratio": "total_output / total_expense"
    }
  },
  "data_sources": [
    {
      "name": "农业统计年鉴",
      "type": "official_statistics",
      "update_frequency": "yearly"
    },
    {
      "name": "农产品价格监测系统",
      "type": "market_data",
      "update_frequency": "monthly"
    }
  ],
  "validation_rules": [
    {
      "field": "total_expense",
      "rule": "value > 0",
      "message": "总支出必须大于0"
    },
    {
      "field": "time_period",
      "rule": "matches_pattern",
      "pattern": "^\\d{4}(-Q[1-4])?$",
      "message": "时间周期格式无效"
    }
  ]
}
